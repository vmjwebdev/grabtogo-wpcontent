{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "frontend-admin/featured-image-field",
  "title": "Featured Image Field",
  "description": "Displays a featured image field.",
  "category": "frontend-admin",
  "textdomain": "frontend-admin",
  "supports": {
    "align": [
      "wide"
    ]
  },
  "attributes": {
    "label": {
      "type": "string",
      "default": "Featured Image"
    },
    "hide_label": {
      "type": "boolean",
      "default": ""
    },
    "required": {
      "type": "boolean",
      "default": ""
    },
    "instructions": {
      "type": "string",
      "default": ""
    },
    "default_value": {
      "type": "string",
      "default": ""
    },
    "placeholder": {
      "type": "string",
      "default": ""
    },
    "prepend": {
      "type": "string",
      "default": ""
    },
    "append": {
      "type": "string",
      "default": ""
    },
    "maxlength": {
      "type": "number",
      "default": ""
    },
    "post_slug": {
      "type": "boolean",
      "default": ""
    }
  },
  "editorScript": "file:../../featured-image/index.js"
}