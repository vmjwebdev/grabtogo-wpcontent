{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "frontend-admin/color-field",
  "title": "Color Field",
  "description": "Displays a color field.",
  "category": "frontend-admin",
  "textdomain": "frontend-admin",
  "supports": {
    "align": [
      "wide"
    ]
  },
  "attributes": {
    "name": {
      "type": "string",
      "default": ""
    },
    "label": {
      "type": "string",
      "default": "Text Field"
