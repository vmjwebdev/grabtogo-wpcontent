      "default": ""
    },
    "required": {
      "type": "boolean",
      "default": ""
    },
    "default_value": {
      "type": "string",
      "default": ""
    },
    "instructions": {
      "type": "string",
      "default": ""
    }
  },
  "editorScript": "file:../../color/index.js"
}